{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "root",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#100e23",
            "background": "#ffe9aa"
          },
          {
            "type": "path",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#ffffff",
            "background": "#1a1af5",
            "properties": {
              "folder_icon": "\uF115",
              "folder_separator_icon": " \uE0B1 ",
              "style": "full"
            }
          },
          {
            "type": "git",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#100e23",
            "background": "#4baf29",
            "properties": {
              "display_status": true,
              "status_colors_enabled": true,
              "local_changes_color": "#FFFF00",
              "behind_color": "#b41b1b",
              "ahead_color": "#96176c"
            }
          },
          {
            "type": "az",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#000000",
            "background": "#9ec3f0",
            "properties": {
              "template": "{{.EnvironmentName}} | {{.Name}}",
              "prefix": " \uFD03 ",
              "include_folders": [
                "*\\\\az"
              ]
            }
          },
          {
            "type": "exit",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#ffffff",
            "background": "#ff8080",
            "properties": {
              "prefix": " \uE20F"
            }
          }
          
        ]
      },
      {
        "type": "prompt",
        "alignment": "right",
        "segments": [
          {
            "type": "text",
            "style": "diamond",
            "foreground": "#fafafa",
            "background": "#2f2f2f",
            "leading_diamond": "\ue0b2",
            "properties": {
              "text": "",
              "prefix": "",
              "postfix": ""
            }
          },
          {
            "type": "dotnet",
            "style": "diamond",
            "foreground": "#3891A6",
            "background": "#2f2f2f",
            "properties": {
              "prefix": " \uE77F ",
              "postfix": "<#7a7a7a> \ue0b3</>"
            }
          },
          {
            "type": "time",
            "style": "diamond",
            "trailing_diamond": "\u2593\ue0c6",
            "foreground": "#fafafa",
            "background": "#2f2f2f",
            "properties": {
              "time_format": "15:04:05",
              "postfix": "<#007ACC> \uf64f </>"
            }
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "#007ACC",
            "properties": {
              "prefix": "",
              "text": "\u03BB"
            }
          }
        ]
      }
    ],
    "final_space": false
  }
  